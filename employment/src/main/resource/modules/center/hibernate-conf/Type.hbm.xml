<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zhangmin.center.entity">
	<class name="Type" table="type">
		<id name="id" column="id" type="java.lang.String" length="36">
			<generator class="uuid.hex"></generator>
		</id>

		<property name="name" column="name" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="40">
		</property>
		<property name="description" column="description" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			>
		</property>
		<property name="score" column="score" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
		length="4">
		</property>
		<property name="maxScore" column="maxScore" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="4">
		</property>
		<property name="flag" column="flag" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="2">
		</property>
	</class>
</hibernate-mapping>

