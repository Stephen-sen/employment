<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zhangmin.center.entity">
	<class name="Student" table="student">
		<id name="id" column="id" type="java.lang.String" length="36">
			<generator class="uuid.hex"></generator>
		</id>

		<property name="name" column="name" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="40">
		</property>
		<property name="age" column="age" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="4">
		</property>
		<property name="address" column="address" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
		>
		</property>
		<property name="birthday" column="birthday" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="30">
		</property>
		<property name="tel" column="tel" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="11">
		</property>
		<property name="email" column="email" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="20">
		</property>
		<property name="major" column="major" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="30">
		</property>
		<property name="assessmentId" column="assessmentId" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="36">
		</property>
		<property name="score" column="score" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="4">
		</property>
		<property name="flag" column="flag" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="2">
		</property>
	</class>
</hibernate-mapping>

