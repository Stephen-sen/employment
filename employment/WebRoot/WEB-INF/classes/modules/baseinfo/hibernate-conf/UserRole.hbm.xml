<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zhangmin.base.entity" >
	<class name="UserRole" table="user_role">
		<id name="id" column="id" type="java.lang.String" length="36">
			<generator class="uuid.hex">
			</generator>
		</id>
		
		<property name="isDefault" column="is_default" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="2">
		</property>
		<property name="userId" column="userId" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="36">
		</property>
		<property name="roleId" column="roleId" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="36">
		</property>
		<property name="flag" column="flag" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="2">
		</property>
		
		<property name="createDate" column="create_date" update="true"
			insert="true" type="java.util.Date" not-null="false" unique="false"
			length="0">
		</property>
		
	</class>
</hibernate-mapping>

